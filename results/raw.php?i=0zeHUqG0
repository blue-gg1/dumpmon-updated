

#include   <stdio.h>


int n;


int dfgd();

////////////////////////////////////////////////////
int main()                                        // 
{

    n = dfgd();
 
    printf("dfgd() = %d \n",  n); 
}



/////////////////////////////////////////////////////
int dfgd()                                         //
{
	
    int n;

	 
    printf("n = %d \n", ::n);
    
    
    if(n < 4) 
	{ 
	    	
       printf("Hello, Anna ! \n"); 
    } 
    printf("Hello ! \n");  
	
return 466;	  	 
	
}












